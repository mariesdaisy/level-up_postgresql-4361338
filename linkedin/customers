  linkedin(# password VARCHAR(50) NOT NULL,
  linkedin(# email VARCHAR(255) UNIQUE NOT NULL,
  linkedin(# created_on TIMESTAMPTZ NOT NULL
  linkedin(# );
  CREATE TABLE
